{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@API/*": ["src/API/*"],
      "@Store/*": ["src/Store/*"],
      "@Root/*": ["src/Root/*"],
      "@Hooks/*": ["src/Hooks/*"],
      "@Pages/*": ["src/Pages/*"],
      "@Styles/*": ["src/Styles/*"],
      "@Routes/*": ["src/Routes/*"],
      "@Assets/*": ["src/Assets/*"],
      "@Helpers/*": ["src/Helpers/*"],
      "@UI/*": ["src/Components/UI/*"],
      "@Middleware/*": ["src/Middleware/*"],
      "@Components/*": ["src/Components/*"],
      "@Constants/*": ["src/CONSTANTS/*"]
    }
  },
  "include": [
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}